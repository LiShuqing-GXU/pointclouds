function D = pairwise(X,varargin)
    c = 1:3
    if numel(varargin)
       if strcmpi( varargin{1}, 'COORD' 
    end
    N = size(X,2);
    D = zeros(N,N);
    for idx = 1:N
        for jdx = idx:N
            D(idx,jdx) = norm( X(c,idx) - X(c,jdx) );
            D(jdx,idx) = D(idx,jdx);
        end
    end
end